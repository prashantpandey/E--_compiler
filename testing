event open (string a1 , int a2 , unsigned int a3);

int a = 2;
int b = a + 4;
int c;

void foo(int h){
    a = h;
    a = h << 2;
    a = h >> a;
    a = (h + 1) + (a + 1);
    a = h + 1;
}
void abc() {
    int g = 2;
    int h;
    string c = "Hello World..!!!";
    h = 100 * g;
    foo(h);
    prt -> g;
    prt -> c;
};

open(file, flags, mode) -> {
    abc();
